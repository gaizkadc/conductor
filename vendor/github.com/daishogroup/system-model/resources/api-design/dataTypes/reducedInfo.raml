    items: !include nodeReducedInfo.raml
    required: true
  descriptors:
    type: array
    items: !include applicationDescriptorReducedInfo.raml
    required: true
  instances:
    type: array
    items: !include applicationInstanceReducedInfo.raml
    required: true
