#%RAML 1.0 DataType

displayName: Reduced Info
description: Simplified dump object.
type: object
properties:
  networks:
    type: array
    items: !include networkReducedInfo.raml
    required: true
  clusters:
    type: array
    items: !include clusterReducedInfo.raml
    required: true
