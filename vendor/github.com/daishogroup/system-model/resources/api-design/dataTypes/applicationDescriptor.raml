#%RAML 1.0 DataType

displayName: ApplicationDescriptor
description: |
An application descriptor contains the information required to deploy and application.
type: object
properties:
  networkId: string
  id: string
  name: string
  description: string
  serviceName: string
  serviceVersion: string
  label: string
  port: integer
