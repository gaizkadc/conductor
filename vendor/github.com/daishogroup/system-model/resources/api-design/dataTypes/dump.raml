#%RAML 1.0 DataType


displayName: Dump
description: Dump of the contents inside the system model.
type: object
properties:
  networks:
    type: array
    items: !include network.raml
    required: true
  clusters:
    type: array
    items: !include cluster.raml
    required: true
  nodes:
    type: array
    items: !include node.raml
    required: true
  descriptors:
    type: array
    items: !include applicationDescriptor.raml
    required: true
  instances:
    type: array
    items: !include applicationInstance.raml
    required: true
